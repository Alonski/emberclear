<FocusCard @title={{t 'ui.setup.overwriteTitle'}}>
  <div class='card-content'>
    <div class='content'>
      {{t 'ui.setup.overwriteQuestion'}}
    </div>
  </div>

  <div class='card-footer'>
    <a
      {{action 'didClickNo'}}
      class='card-footer-item'>
      {{t 'ui.setup.overwriteAbort'}}
    </a>
    <a
      {{action 'didClickYes'}}
      class='card-footer-item'>
      {{t 'ui.setup.overwriteConfirm'}}
    </a>
  </div>
</FocusCard>
