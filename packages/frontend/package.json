    "build:production": "EXPERIMENTAL_RENDER_MODE_SERIALIZE=true yarn build --environment production",
    "start": "yarn ember serve -p 4201",
    "start:dev": "SW_DISABLED=true FASTBOOT_DISABLED=true yarn start -p 4201",
    "test": "yarn ember test",
    "lint": "bash -c 'shopt -s globstar && yarn eslint config lib server tests/**/*.ts src/**/*.ts'",
    "lint:fix": "bash -c 'yarn lint --fix'",
    "deploy": "yarn ember deploy production"
